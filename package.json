    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "bootstrap": "^5.2.0",
    "compression": "^1.7.3",
    "ejs": "^3.1.8",
    "express": "^4.16.4",
    "fs": "^0.0.1-security",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mathjs": "^10.5.2",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "react-bootstrap": "^2.4.0",
    "sequelize": "^6.3.5",
    "socketio": "^1.0.0",
    "use-places-autocomplete": "^3.0.0",
    "util": "^0.12.4",
    "utils": "^0.3.1",
    "victory": "^36.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
